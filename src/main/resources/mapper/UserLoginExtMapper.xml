<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qyys.quickapp.repository.mapper.UserLoginMapper">
    <delete id="deleteByUserId" parameterType="java.lang.String">
      delete from dish_user_login
      where user_id = #{userId,jdbcType=VARCHAR}
    </delete>

    <select id="selectByToken" parameterType="java.lang.String" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from dish_user_login
        where login_token = #{xToken,jdbcType=VARCHAR}
    </select>
</mapper>